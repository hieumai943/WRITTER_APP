<div id="container">
    <div id="saving">
        <div class="title">
            <h3>My files</h3>
            <i class="searchFile fa fa-search" aria-hidden="true"></i>
        </div>
        <a href="/news" style="color: #000">
        <div id="addFile">
            <i class="fa fa-plus-circle" aria-hidden="true"></i>
            <span>New File</span>
        </div>
       
    </a>
    
        <div class="storageFile">
             {{#each courses}}
         <div class="file">
        <a href="/news/{{this.slug}}"  onclick="completed(${index})"><i class="fa fa-folder-open-o" aria-hidden="true" style="font-size:50px;"></i></a>
        <span>{{this.inputVal}}</span>
        <span id='date' >DATE: ${day} / ${month} / ${year}</span>
        <i id="xoa"class="fa fa-trash-o" aria-hidden="true" onclick="deleteFile(${index})" style="font-size:30px;"></i>
        </div>
       {{/each}}
        </div>
    </div>
  
</div>

    <script src="menu.js">

    </script>